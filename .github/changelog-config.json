{
    "categories": [
        {
            "title": "## ğŸš€ Features",
            "labels": [
                "feature",
                "enhancement"
            ]
        },
        {
            "title": "## ğŸ› Bug Fixes",
            "labels": [
                "bug",
                "fix"
            ]
        },
        {
            "title": "## ğŸ§ª Tests",
            "labels": [
                "test",
                "testing"
            ]
        },
        {
            "title": "## ğŸ“š Documentation",
            "labels": [
                "documentation",
                "docs"
            ]
        },
        {
            "title": "## ğŸ”§ Maintenance",
            "labels": [
                "maintenance",
                "chore",
                "dependencies"
            ]
        }
    ],
    "ignore_labels": [
        "ignore",
        "wontfix",
        "duplicate",
        "invalid"
    ],
    "sort": "ASC",
    "template": "${{CHANGELOG}}",
    "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
    "empty_template": "- No changes",
    "label_extractor": [
        {
            "pattern": "^feat:",
            "target": "feature"
        },
        {
            "pattern": "^fix:",
            "target": "bug"
        },
        {
            "pattern": "^test:",
            "target": "test"
        },
        {
            "pattern": "^docs:",
            "target": "documentation"
        },
        {
            "pattern": "^chore:",
            "target": "maintenance"
        }
    ]
}